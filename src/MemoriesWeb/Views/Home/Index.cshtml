                <div class="col-1">
                </div>
                <div class="col-6">
                    <div class="sidedevices">
                        <div class="iphoneipad2">
                            <div class="iphone">
                                <div class="mask">
                                    <img class="mask-img" src="~/images/mobileapp.svg">
